      "keyColor": "#b43737"
    },
    {
      "type": "custom",
      "format": "\u001b[90m└────────────────────────────────────────────────────┘"
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[90m┌──────────────────────Software──────────────────────┐"
    },
    {
      "type": "os",
      "key": " OS",
      "keyColor": "#8b1e1e"
    },
    {
      "type": "kernel",
      "key": "│ ├",
      "keyColor": "#8b1e1e"
