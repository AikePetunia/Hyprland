  },
  "modules": [
    "break",
    {
      "type": "custom",
      "format": "\u001b[90m┌──────────────────────Hardware──────────────────────┐"
    },
    {
      "type": "custom",
      "format": "",
      "key": " PC",
      "keyColor": "#b43737"
    },
    {
      "type": "cpu",
      "key": "│ ├",
      "keyColor": "#b43737"
    },
    {
      "type": "gpu",
      "key": "│ ├󰍛",
